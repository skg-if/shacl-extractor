schema:AudienceShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:audienceType ] ;
    sh:targetClass schema:Audience .

